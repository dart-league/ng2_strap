    "RatingDemo": {
      "class": "Directive",
      "isComponent": true,
      "selector": "rating-demo",
      "exportAs": null,
      "type": {
        "class": "Type",
        "name": "RatingDemo",
        "moduleUrl": "asset:ng2_strap/web/components/rating/rating_demo.dart",
