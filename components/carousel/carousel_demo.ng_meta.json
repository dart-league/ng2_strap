      "inputs": {},
      "outputs": {},
      "hostListeners": {},
      "hostProperties": {},
      "hostAttributes": {},
      "lifecycleHooks": [],
      "template": {
        "encapsulation": 2,
        "template": "<div>\n  <div>\n    <n2s-carousel [interval]=\"myIntervalInMs\" [noWrap]=\"noWrapSlides\">\n      <n2s-slide *ngFor=\"let slide of slides; let index=index\"\n             [active]=\"slide['active'] != null ? slide['active'] : false \">\n        <img [src]=\"slide['image']\">\n\n        <div class=\"carousel-caption\">\n          <h4>Slide {{index}}</h4>\n\n          <p>{{slide['text']}}</p>\n        </div>\n      </n2s-slide>\n    </n2s-carousel>\n  </div>\n  <br>\n  <div>\n    <button type=\"button\" class=\"btn btn-info\"\n            (click)=\"addSlide()\">Add Slide\n    </button>\n    <!-- issue: removing currently active slide should not lead to white screen? -->\n    <!--<button type=\"button\" class=\"btn btn-info\"-->\n            <!--(click)=\"removeSlide(2)\">Remove Slide#3-->\n    <!--</button>-->\n    <br>\n\n    <div class=\"checkbox\">\n      <label>\n        <input type=\"checkbox\" [(ngModel)]=\"noWrapSlides\">\n        Disable Slide Looping\n      </label>\n    </div>\n\n    Interval, in seconds: <input type=\"number\" class=\"form-control\" [(ngModel)]=\"myIntervalInSeconds\">\n    <br/>Enter a negative number or 0 to stop the interval.\n  </div>\n</div>\n",
        "templateUrl": "asset:ng2_strap/web/components/carousel/carousel_demo.html",
        "styles": [],
        "styleUrls": [],
        "ngContentSelectors": []
      },
      "providers": [],
