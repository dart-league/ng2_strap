  "type": {
    "CarouselDemo": {
      "class": "Directive",
      "isComponent": true,
      "selector": "carousel-demo",
      "exportAs": null,
      "type": {
        "class": "Type",
        "name": "CarouselDemo",
        "moduleUrl": "asset:ng2_strap/web/components/carousel/carousel_demo.dart",
        "prefix": null,
